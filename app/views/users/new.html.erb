<h1>New User</h1>

 <%if @user.errors.any?%>
  <ul>
    <%@user.errors.full_messages.each do |message|%>
    <li class="notice"><strong><%=message%></strong></li>
      <%end%>
  </ul>
  <%end%><br>


<%= form_for @user do |f| %>
<%= f.label :name %>:
  <%= f.text_field :name %><br><br>

  <%= f.label :email %>:
  <%= f.text_field :email %><br><br>


  Password: <%= f.password_field :password %><br><br>
  



<%= f.submit "Create User" %>
<% end %>