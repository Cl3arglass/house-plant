<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
  <head>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
    <title>HousePlant</title>

    

    
    <div class="topnav">
      
      <a class="active" href="<%= root_url %>"/>Home</a>
      <% if !current_user %>
        <a href="<%= new_user_path %>"/>Sign Up</a>
        <a href="<%= signin_path %>">Sign In</a>
      <% end %>
      <% if current_user %>
        <a href="<%= user_plants_path(current_user) %>">My Account</a>
        <a href="<%= new_user_plant_path(current_user) %>">Add New Plant</a>
        <a href="<%= comments_index_path %>">View All Comments</a>
        <a href="<%= logout_path %>">Log Out</a>
      <% end %>
      
    </div>
  </head>
  <body>
    <div class="wrapper">



        

        <%= yield %>


      <footer class="branding">
        <small>&copy; 2018 <strong>HousePlant</strong></small>
      </footer>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>
    <!--[if lt IE 7]>
      <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
      <script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
    <![endif]-->
  </body>
</html>
